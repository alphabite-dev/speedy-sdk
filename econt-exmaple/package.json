{
  "name": "econt",
  "version": "1.0.0",
  "description": "TypeScript SDK for Econt shipping API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "econt",
    "shipping",
    "bulgaria",
    "sdk",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/ui": "^4.0.3",
    "dotenv": "^17.2.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.3"
  }
}
